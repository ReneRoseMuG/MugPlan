{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "tsx script/build.ts",
    "start:orig": "NODE_ENV=production node dist/index.cjs",
    "start": "NODE_ENV=production node --env-file=.env dist/index.cjs",
    "check": "npm run check:encoding:frontend && tsc",
    "typecheck": "tsc --noEmit",
    "lint": "cross-env ESLINT_USE_FLAT_CONFIG=false eslint client/src server shared --ext .ts,.tsx",
    "audit": "npm audit --audit-level=moderate",
    "secrets": "powershell -NoProfile -ExecutionPolicy Bypass -File script/run-gitleaks.ps1 detect --source . --no-git",
    "review": "npm run typecheck && npm run lint && npm run audit && npm run secrets",
    "check:encoding:frontend": "tsx script/check-frontend-encoding.ts",
    "db:push": "drizzle-kit push",
    "db:migrate:project-names": "tsx script/migrate-project-names-with-customer-number.ts",
    "db:seed:roles": "tsx script/seed-roles.ts",
    "test:template-render": "tsx script/test-template-render.ts",
    "verify:demo-seed": "tsx script/verify-demo-seed.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:extraction:mock": "vitest run tests/integration/server/documentExtraction.routes.test.ts tests/unit/validation/extractionValidator.structure.test.ts tests/unit/services/documentProcessing.customerResolution.test.ts tests/unit/ui/projectForm.documentExtractionFlow.test.tsx tests/unit/ui/appointmentForm.documentExtractionFlow.test.tsx tests/unit/extraction/documentTextExtractor.test.ts tests/unit/validation/dtoValidators.test.ts",
    "test:extraction:live": "vitest run tests/integration/server/documentExtraction.routes.liveAi.test.ts tests/integration/extraction/documentExtraction.livePipeline.fixture.test.ts",
    "test:extraction": "npm run test:extraction:mock && npm run test:extraction:live",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage"
  },
